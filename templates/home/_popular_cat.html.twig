<div class="col-lg-12">
  <div class="card-deck">
    {% for cat in cats %}
      <div class="card category">
        <div class="inner">
          <img class="card-img-top category" src="https://images.unsplash.com/photo-1586611961063-ddc490f9fb51?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=1050&q=80" alt="Card image cap"/>
        </div> 
        <img src="https://randomuser.me/api/portraits/women/56.jpg" alt="" class="card_category-avatar"/>
        <div class="card-body">
          <h5 class="card-title category text-center">
            <a href="#">{{ cat.title }}</a>
          </h5>
          <p class="text-center">
            <a href="#" class="btn category_browse">Parcourir</a>
          </p>
        </div>
      </div>
    {% endfor %}
  </div>
</div>